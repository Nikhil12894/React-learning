{
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "redhat.telemetry.enabled": true,
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "explorer.confirmDelete": false,
    "yaml.schemas": {
        "kubernetes": "*.yaml",
        "dockerComposeFile": "*.yaml"
    },
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "editor.fontSize": 20,
    "terminal.integrated.fontSize": 20,
    "terminal.integrated.fontFamily": "monospace",
    "workbench.colorCustomizations": {
        "terminal.backsround": "#070707",
        "terminal.selectionBackground": "#c3f7a7ee",
        "terminal.selectionForeground": "#000000",
        "tab.activeBorder": "#FFFFFF",
        "tab.activeBackground": "#000000",
        "tab.activeBorderTop": "#FFFFFF",
        "tab.activeForeground": "#FFFFFF"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#80fce183"
    },
    "editor.codeActionsOnSave": {},
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Default Dark Modern",
    "editor.fontFamily": "monospace",
    "editor.wordWrap": "bounded",
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "#000000",
        "background": "#FFFFFF",
        "opacity": 50,
        "iconColour": "#456ecc",
        "gutterIcon": true
    },
    "todo-tree.highlights.customHighlight": {
        "TODO:": {
            "icon": "checkbox",
            "foreground": "#000000",
            "background": "#f0e067",
            "iconColour": "#f0e067"
        },
        "NOTE:": {
            "icon": "note",
            "foreground": "#FFFFFF",
            "background": "#9acfeb",
            "iconColour": "#9acfeb"
        },
        "USEFUL:": {
            "icon": "verified",
            "foreground": "#000000",
            "background": "#66ddaa",
            "iconColour": "#66ddaa"
        },
        "COMMENT:": {
            "icon": "comment",
            "foreground": "#FFFFFF",
            "background": "#686869",
            "iconColour": "#FFFFFF"
        },
        "LEARN:": {
            "icon": "bookmark",
            "foreground": "#FFFFFF",
            "background": "#ed72c6",
            "iconColour": "#ed72c6"
        },
        "FIXME:": {
            "icon": "tools",
            "foreground": "#dc143c",
            "background": "#deb887",
            "iconColour": "#deb887"
        },
        "RECHECK:": {
            "icon": "codescan",
            "foreground": "#FFFFFF",
            "background": "#7B3F00",
            "iconColour": "#7B3F00"
        },
        "INCOMPLETE:": {
            "icon": "alert",
            "foreground": "#FFFFFF",
            "background": "#C71585",
            "iconColour": "#C71585"
        },
        "BUG:": {
            "icon": "bug",
            "foreground": "#FFFFFF",
            "background": "#dc143c",
            "iconColour": "#dc143c"
        },
        "SEE NOTES:": {
            "icon": "note",
            "foreground": "#FFFFFF",
            "background": "#008080",
            "iconColour": "#008080"
        },
        "POST:": {
            "icon": "share",
            "foreground": "#FFFFFF",
            "background": "#02b526",
            "iconColour": "#02b526"
        },
        "[ ]:": {
            "icon": "check",
            "foreground": "#000000",
            "background": "#FFFFFF",
            "iconColour": "#f0e067"
        },
        "[x]:": {
            "icon": "check",
            "foreground": "#FFFFFF",
            "background": "#02b526",
            "iconColour": "#02b526"
        },
        "ANOTHER WAY:": {
            "icon": "issue-reopened",
            "foreground": "#ffffff",
            "background": "#4eecf5",
            "iconColour": "#4eecf5"
        }
    },
    "todo-tree.general.tags": [
        "BUG:",
        "SEE NOTES:",
        "HACK:",
        "FIXME:",
        "RECHECK:",
        "INCOMPLETE:",
        "TODO:",
        "NOTE:",
        "POST:",
        "USEFUL:",
        "LEARN:",
        "COMMENT:",
        "[ ]:",
        "[x]:",
        "ANOTHER WAY:"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "cSpell.language": "en"
}